    <string notr="true">/* /////////////////////////////////////////////////////////////////////////////////////////////////

APP STYLES - DARK THEME

///////////////////////////////////////////////////////////////////////////////////////////////// */

QWidget{
    color: #dddddd;
    font: 10pt &quot;Segoe UI&quot;;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
    color: #ffffff;
    background-color: rgba(33, 37, 43, 180);
    border: 1px solid #2c313a;
    background-image: none;
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 2px solid #6F99CB; /* Açık Mavi */
    text-align: left;
    padding-left: 8px;
    margin: 0px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Bg App */
#bgApp {    
    background-color: #282c34;
    border: 1px solid #2c313a;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Left Menu */
#leftMenuBg {   
    background-color: #21252b;
}
#topLogo {
    background-color: #21252b;
    background-image: url(:/images/images/images/edubot_kare_logo.png);
    background-position: centered;
    background-repeat: no-repeat;
}
#titleLeftApp {
    font: 20pt &quot;Bahn Pro&quot;, 12pt &quot;Segoe UI&quot;;
    letter-spacing: 4px;
}
#titleLeftDescription {
    font: 12pt &quot;Caveat Brush&quot;, 8pt &quot;Segoe UI&quot;;
    letter-spacing: 0.5px;
    color: #F96A76; /* Kırmızı */
}

/* MENUS */
#topMenu .QPushButton { 
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 22px solid transparent;
    background-color: transparent;
    text-align: left;
    padding-left: 44px;
}
#topMenu .QPushButton:hover {
    background-color: #282c34;
}
#topMenu .QPushButton:pressed { 
    background-color: #3F72AF; /* Mavi */
    color: #FFFFFF;
}
#bottomMenu .QPushButton {  
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 20px solid transparent;
    background-color:transparent;
    text-align: left;
    padding-left: 44px;
}
#bottomMenu .QPushButton:hover {
    background-color: #282c34;
}
#bottomMenu .QPushButton:pressed {  
    background-color: #3F72AF; /* Mavi */
    color: #FFFFFF;
}
#leftMenuFrame{
    border-top: 3px solid #2C313A;
}

/* Toggle Button */
#toggleButton {
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 20px solid transparent;
    background-color: #252930;
    text-align: left;
    padding-left: 44px;
    color: #717E95;
}
#toggleButton:hover {
    background-color: #282C34;
}
#toggleButton:pressed {
    background-color: #3F72AF; /* Mavi */
}

/* Title Menu */
#titleRightInfo { padding-left: 10px; }

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Extra Tab */
#extraLeftBox { 
    background-color: #2C313A;
}
#extraTopBg{    
    background-color: #3F72AF /* Mavi */
}

/* Icon */
#extraIcon {
    background-position: center;
    background-repeat: no-repeat;
    background-image: url(:/icons/images/icons/people-fill.svg);
}

/* Label */
#extraLabel { color: #FFFFFF; }

/* Btn Close */
#extraCloseColumnBtn { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#extraCloseColumnBtn:hover { background-color: #6F99CB; border-style: solid; border-radius: 4px; } /* Açık Mavi */
#extraCloseColumnBtn:pressed { background-color: #38669D; border-style: solid; border-radius: 4px; } /* Mavi (Bir tık koyu) */

/* Extra Content */
#extraContent{
    border-top: 3px solid #282C34;
}

/* Extra Top Menus */
#extraTopMenu .QPushButton {
    border: none;
    border-left: 22px solid transparent;
    background-color:transparent;
    text-align: left;
}
#extraTopMenu .QPushButton:hover {
    background-color: #282C34;
}
#extraTopMenu .QPushButton:pressed {    
    background-color: #3F72AF; /* Mavi */
    color: #FFFFFF;
}

/* Add Teacher Button */
#btnAddTeacher {
    border: 2px solid #112D4E;
    border-radius: 5px; 
    background-color: #3F72AF;
	padding: 3px;
	margin: 5px 15px 20px 15px;
}
#btnAddTeacher:hover {
    background-color: #6F99CB;
    border: 2px solid #3F72AF;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Content App */
#contentTopBg{  
    background-color: #21252B;
}
#contentBottom{
    border-top: 3px solid #2C313A;
}

/* Top Buttons */
#rightButtons .QPushButton { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#rightButtons .QPushButton:hover { background-color: #2C3139; border-style: solid; border-radius: 4px; }
#rightButtons .QPushButton:pressed { background-color: #171A1E; border-style: solid; border-radius: 4px; }

/* Theme Settings */
#extraRightBox { background-color: #2C313A; }
#themeSettingsTopDetail { background-color: #3F72AF; } /* Mavi */

/* Bottom Bar */
#bottomBar { background-color: #2C313A; }
#bottomBar QLabel { font-size: 11px; color: #717E95; padding-left: 10px; padding-right: 10px; padding-bottom: 2px; }

/* Frame Size Grip */
#frame_size_grip {
    background-image: url(:/icons/images/icons/cil-size-grip.png);
    background-position: right bottom;
    background-repeat: no-repeat;
}

/* CONTENT SETTINGS */
/* MENUS */
#contentSettings .QPushButton { 
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 22px solid transparent;
    background-color:transparent;
    text-align: left;
    padding-left: 44px;
}
