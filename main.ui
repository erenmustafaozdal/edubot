    <string notr="true">/* /////////////////////////////////////////////////////////////////////////////////////////////////

APP STYLES - DARK THEME

///////////////////////////////////////////////////////////////////////////////////////////////// */

QWidget{
    color: #dddddd;
    font: 10pt &quot;Segoe UI&quot;;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
    color: #ffffff;
    background-color: rgba(33, 37, 43, 180);
    border: 1px solid #2c313a;
    background-image: none;
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 2px solid #6F99CB; /* Açık Mavi */
    text-align: left;
    padding-left: 8px;
    margin: 0px;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Bg App */
#bgApp {    
    background-color: #282c34;
    border: 1px solid #2c313a;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Left Menu */
#leftMenuBg {   
    background-color: #21252b;
}
#topLogo {
    background-color: #21252b;
    background-image: url(:/images/images/images/edubot_kare_logo.png);
    background-position: centered;
    background-repeat: no-repeat;
}
#titleLeftApp {
    font: 20pt &quot;Bahn Pro&quot;, 12pt &quot;Segoe UI&quot;;
    letter-spacing: 4px;
}
#titleLeftDescription {
    font: 12pt &quot;Caveat Brush&quot;, 8pt &quot;Segoe UI&quot;;
    letter-spacing: 0.5px;
    color: #F96A76; /* Kırmızı */
}

/* MENUS */
#topMenu .QPushButton { 
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 22px solid transparent;
    background-color: transparent;
    text-align: left;
    padding-left: 44px;
}
#topMenu .QPushButton:hover {
    background-color: #282c34;
}
#topMenu .QPushButton:pressed { 
    background-color: #3F72AF; /* Mavi */
    color: #FFFFFF;
}
#bottomMenu .QPushButton {  
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 20px solid transparent;
    background-color:transparent;
    text-align: left;
    padding-left: 44px;
}
#bottomMenu .QPushButton:hover {
    background-color: #282c34;
}
#bottomMenu .QPushButton:pressed {  
    background-color: #3F72AF; /* Mavi */
    color: #FFFFFF;
}
#leftMenuFrame{
    border-top: 3px solid #2C313A;
}

/* Toggle Button */
#toggleButton {
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 20px solid transparent;
    background-color: #252930;
    text-align: left;
    padding-left: 44px;
    color: #717E95;
}
#toggleButton:hover {
    background-color: #282C34;
}
#toggleButton:pressed {
    background-color: #3F72AF; /* Mavi */
}

/* Title Menu */
#titleRightInfo { padding-left: 10px; }

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Extra Tab */
#teachersBox { 
    background-color: #2C313A;
}
#extraTopBg{    
    background-color: #3F72AF /* Mavi */
}

/* Icon */
#extraIcon {
    background-position: center;
    background-repeat: no-repeat;
    background-image: url(:/icons/images/icons/people-fill.svg);
}

/* Label */
#extraLabel { color: #FFFFFF; }

/* Btn Close */
#teachersBoxCloseBtn { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#teachersBoxCloseBtn:hover { background-color: #6F99CB; border-style: solid; border-radius: 4px; } /* Açık Mavi */
#teachersBoxCloseBtn:pressed { background-color: #38669D; border-style: solid; border-radius: 4px; } /* Mavi (Bir tık koyu) */

/* Extra Content */
#extraContent{
    border-top: 3px solid #282C34;
}

/* Extra Top Menus */
#extraTopMenu .QPushButton {
    border: none;
    border-left: 22px solid transparent;
    background-color:transparent;
    text-align: left;
}
#extraTopMenu .QPushButton:hover {
    background-color: #282C34;
}
#extraTopMenu .QPushButton:pressed {    
    background-color: #3F72AF; /* Mavi */
    color: #FFFFFF;
}

/* Add Teacher Button */
#btn_add_teacher {
    border: 2px solid #112D4E;
    border-radius: 5px; 
    background-color: #3F72AF;
	padding: 3px;
	margin: 5px 15px 20px 15px;
}
#btn_add_teacher:hover {
    background-color: #6F99CB;
    border: 2px solid #3F72AF;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Content App */
#contentTopBg{  
    background-color: #21252B;
}
#contentBottom{
    border-top: 3px solid #2C313A;
}

/* Top Buttons */
#rightButtons .QPushButton { background-color: rgba(255, 255, 255, 0); border: none;  border-radius: 5px; }
#rightButtons .QPushButton:hover { background-color: #2C3139; border-style: solid; border-radius: 4px; }
#rightButtons .QPushButton:pressed { background-color: #171A1E; border-style: solid; border-radius: 4px; }

/* Theme Settings */
#extraRightBox { background-color: #2C313A; }
#themeSettingsTopDetail { background-color: #3F72AF; } /* Mavi */

/* Bottom Bar */
#bottomBar { background-color: #2C313A; }
#bottomBar QLabel { font-size: 11px; color: #717E95; padding-left: 10px; padding-right: 10px; padding-bottom: 2px; }

/* Frame Size Grip */
#frame_size_grip {
    background-image: url(:/icons/images/icons/cil-size-grip.png);
    background-position: right bottom;
    background-repeat: no-repeat;
}

/* CONTENT SETTINGS */
/* MENUS */
#contentSettings .QPushButton { 
    background-position: left center;
    background-repeat: no-repeat;
    border: none;
    border-left: 22px solid transparent;
    background-color:transparent;
    text-align: left;
    padding-left: 44px;
}
#contentSettings .QPushButton:hover {
    background-color: #282C34;
}
#contentSettings .QPushButton:pressed { 
    background-color: #3F72AF; /* Mavi */
    color: #FFFFFF;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
QTableWidget */
QTableWidget {  
    background-color: transparent;
    padding: 10px;
    border-radius: 5px;
    gridline-color: #2C313A;
    border-bottom: 1px solid #2C313C;
}
QTableWidget::item{
    border-color: #2C313C;
    padding-left: 5px;
    padding-right: 5px;
    gridline-color: #2C313C;
}
QTableWidget::item:selected{
    background-color: #3F72AF; /* Mavi */
}
QHeaderView::section{
    background-color: #21252B;
    max-width: 30px;
    border: 1px solid #2C313A;
    border-style: none;
    border-bottom: 1px solid #2C313C;
    border-right: 1px solid #2C313C;
}
QTableWidget::horizontalHeader {    
    background-color: #21252B;
}
QHeaderView::section:horizontal
{
    border: 1px solid #21252B;
    background-color: #21252B;
    padding: 3px;
    border-top-left-radius: 7px;
    border-top-right-radius: 7px;
}
QHeaderView::section:vertical
{
    border: 1px solid #2C313C;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
LineEdit */
QLineEdit {
    background-color: #21252B;
    border-radius: 5px;
    border: 2px solid #21252B;
    padding-left: 10px;
    selection-color: #FFFFFF;
    selection-background-color: #6F99CB;
}
QLineEdit:hover {
    border: 2px solid #404758;
}
QLineEdit:focus {
    border: 2px solid #5B657C;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
PlainTextEdit */
QPlainTextEdit {
    background-color: #21252B;
    border-radius: 5px;
    padding: 10px;
    selection-color: #FFFFFF;
    selection-background-color: #6F99CB;
}
QPlainTextEdit  QScrollBar:vertical {
    width: 8px;
 }
QPlainTextEdit  QScrollBar:horizontal {
    height: 8px;
 }
QPlainTextEdit:hover {
    border: 2px solid #404758;
}
QPlainTextEdit:focus {
    border: 2px solid #5B657C;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
ScrollBars */
QScrollBar:horizontal {
    border: none;
    background: #343B48;
    height: 8px;
    margin: 0px 21px 0 21px;
    border-radius: 0px;
}
QScrollBar::handle:horizontal {
    background: #3F72AF; /* Mavi */
    min-width: 25px;
    border-radius: 4px
}
QScrollBar::add-line:horizontal {
    border: none;
    background: #373F4D;
    width: 20px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
    border: none;
    background: #373F4D;
    width: 20px;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal
{
     background: none;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
     background: none;
}
 QScrollBar:vertical {
    border: none;
    background: #343B48;
    width: 8px;
    margin: 21px 0 21px 0;
    border-radius: 0px;
 }
 QScrollBar::handle:vertical {  
    background: #3F72AF; /* Mavi */
    min-height: 25px;
    border-radius: 4px
 }
 QScrollBar::add-line:vertical {
     border: none;
    background: #373F4D;
     height: 20px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }
 QScrollBar::sub-line:vertical {
    border: none;
    background: #373F4D;
     height: 20px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }
 QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     background: none;
 }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }

/* /////////////////////////////////////////////////////////////////////////////////////////////////
CheckBox */
QCheckBox::indicator {
    border: 3px solid #343B48;
    width: 15px;
    height: 15px;
    border-radius: 10px;
    background: #2C313C;
}
QCheckBox::indicator:hover {
    border: 3px solid #3A4251;
}
QCheckBox::indicator:checked {
    background: 3px solid #343B48;
    border: 3px solid #343B48;  
    background-image: url(:/icons/images/icons/cil-check-alt.png);
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
RadioButton */
QRadioButton::indicator {
    border: 3px solid #343B48;
    width: 15px;
    height: 15px;
    border-radius: 10px;
    background: #2C313C;
}
QRadioButton::indicator:hover {
    border: 3px solid #3A4251;
}
QRadioButton::indicator:checked {
    background: 3px solid #5E6A82;
    border: 3px solid #343B48;  
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
ComboBox */
QComboBox{
    background-color: #1B1D23;
    border-radius: 5px;
    border: 2px solid #21252B;
    padding: 5px;
    padding-left: 10px;
}
QComboBox:hover{
    border: 2px solid #404758;
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 25px; 
    border-left-width: 3px;
    border-left-color: rgba(39, 44, 54, 150);
    border-left-style: solid;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;    
    background-image: url(:/icons/images/icons/caret-down-fill.svg);
    background-position: center;
    background-repeat: no-reperat;
 }
QComboBox QAbstractItemView {
    color: #6F99CB;  
    background-color: #21252B;
    padding: 10px;
    selection-background-color: #272C36;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Sliders */
QSlider::groove:horizontal {
    border-radius: 5px;
    height: 10px;
    margin: 0px;
    background-color: #343B48;
}
QSlider::groove:horizontal:hover {
    background-color: #373E4C;
}
QSlider::handle:horizontal {
    background-color: #3F72AF; /* Mavi */
    border: none;
    height: 10px;
    width: 10px;
    margin: 0px;
    border-radius: 5px;
}
QSlider::handle:horizontal:hover {
    background-color: #5280B7; /* Mavi (Bir tık açık)  */
}
QSlider::handle:horizontal:pressed {
    background-color: #6F99CB;
}

QSlider::groove:vertical {
    border-radius: 5px;
    width: 10px;
    margin: 0px;
    background-color: #343B48;
}
QSlider::groove:vertical:hover {
    background-color: #373E4C;
}
QSlider::handle:vertical {
    background-color: #3F72AF; /* Mavi */
    border: none;
    height: 10px;
    width: 10px;
    margin: 0px;
    border-radius: 5px;
}
QSlider::handle:vertical:hover {
    background-color: #5280B7; /* Mavi (Bir tık açık)  */
}
QSlider::handle:vertical:pressed {
    background-color: #6F99CB;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
CommandLinkButton */
QCommandLinkButton {    
    color: #6F99CB;
    border-radius: 5px;
    padding: 5px;
    color: #6F99CB; /* Açık Mavi */
}
QCommandLinkButton:hover {  
    color: #6F99CB; /* Açık Mavi */
    background-color: #2C313C;
}
QCommandLinkButton:pressed {    
    color: #3F72AF; /* Mavi */
    background-color: #343A47;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
Button */
#pagesContainer QPushButton {
	border: 2px solid#343B48;
	border-radius: 5px;	
	background-color: #343B48;
}
#pagesContainer QPushButton:hover {
	background-color:#394150;
	border: 2px solid #3D4656;
}
#pagesContainer QPushButton:pressed {	
	background-color: #232831;
	border: 2px solid#2B323D;
}

/* /////////////////////////////////////////////////////////////////////////////////////////////////
QGroupBox */
QGroupBox {
    border: 1px solid #5B657C;
    border-radius: 9px;
    margin-top: 20px;
    margin-bottom: 20px;
	font-size: 13px;
    font-weight: bold;
	background-color: #21252b;
	color: #F96A76;
}

QGroupBox::title {
    margin-top: 10px;
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 3px 0 3px;
}

